	Eprint = {http://mcb.asm.org/content/24/17/7795.full.pdf+html},
	Journal = {Molecular and Cellular Biology},
	Number = {17},
	Pages = {7795-7805},
	Title = {Spliced-Leader RNA trans Splicing in a Chordate, Oikopleura dioica, with a Compact Genome},
	Url = {http://mcb.asm.org/content/24/17/7795.abstract},
	Volume = {24},
