	Day = {01},
	Doi = {10.1007/s00427-003-0322-z},
	Issn = {1432-041X},
	Journal = {Development Genes and Evolution},
	Month = {Jun},
	Number = {5},
	Pages = {235--244},
	Title = {A genomewide survey of developmentally relevant genes in Ciona intestinalis},
	Url = {https://doi.org/10.1007/s00427-003-0322-z},
	Volume = {213},
	Year = {2003},
