	Journal = {RNA},
	Number = {7},
	Pages = {714-723},
	Title = {Most mRNAs in the nematode Ascaris lumbricoides are trans-spliced: a role for spliced leader addition in translational efficiency.},
	Url = {http://rnajournal.cshlp.org/content/1/7/714.abstract},
	Volume = {1},
