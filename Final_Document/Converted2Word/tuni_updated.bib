	Issn = {19326203},
	Journal = {PLoS ONE},
	Number = {9},
	Pages = {1--11},
	Pmid = {22957063},
	Title = {{Prediction of Conserved Precursors of miRNAs and Their Mature Forms by Integrating Position-Specific Structural Features}},
	Volume = {7},
