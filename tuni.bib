isbn = {1545-9993},
issn = {1545-9993},
journal = {Nature Structural {\&} Molecular Biology},
number = {2},
pages = {183--189},
pmid = {19151725},
title = {{A distinct class of small RNAs arises from pre-miRNA–proximal regions in a simple chordate}},
url = {http://www.nature.com/doifinder/10.1038/nsmb.1536},
volume = {16},
year = {2009}
}
