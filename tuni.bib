issn = {1465-6906},
journal = {Genome Biol},
keywords = {Animals,Base Sequence,Ciona intestinalis/genetics,Computational Biology/*methods,Genome,High-Throughput Screening Assays,MicroRNAs/*analysis/chemistry,Molecular Sequence Data,Phylogeny,Sequence Analysis, RNA/*methods},
number = {4},
pages = {R39},
pmid = {20370911},
title = {{miRTRAP, a computational method for the systematic identification of miRNAs from high throughput sequencing data}},
