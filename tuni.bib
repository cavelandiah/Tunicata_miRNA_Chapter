@article{Kusakabe2013,
abstract = {Muscle-specific miR-1/206 and miR-133 families have been suggested to play fundamental roles in skeletal and cardiac myogenesis in vertebrates. To gain insights into the relationships between the divergence of these miRs and muscular tissue types, we investigated the expression patterns of miR-1 and miR-133 in two ascidian Ciona species and compared their genomic structures with those of other chordates. We found that Ciona intestinalis and Ciona savignyi each possess a single copy of the miR-1/miR-133 cluster, which is only 350 nucleotide long. During embryogenesis, Ciona miR-1 and miR-133 are generated as a single continuous primary transcript accumulated in the nuclei of the tail muscle cells, starting at the gastrula stage. In adults, mature miR-133 and miR-1 are differentially expressed in the heart and body wall muscle. Expression of the reporter gene linked to the 850-bp upstream region of the predicted transcription start site confirmed that this region drives the muscle-specific expression of the primary transcript of miR-1/miR-133. In many deuterostome lineages, including that of Ciona, the miR-1/133 cluster is located in the same intron of the mind bomb (mib) gene in reverse orientation. Our results suggest that the origin of genomic organization and muscle-specific regulation of miR-1/133 can be traced back to the ancestor of chordates. Duplication of this miR cluster might have led to the remarkable elaboration in the morphology and function of skeletal muscles in the vertebrate lineage. {\textcopyright} 2012 Elsevier B.V. All rights reserved.},
author = {Kusakabe, Rie and Tani, Saori and Nishitsuji, Koki and Shindo, Miyuki and Okamura, Kohji and Miyamoto, Yuki and Nakai, Kenta and Suzuki, Yutaka and Kusakabe, Takehiro G. and Inoue, Kunio},
doi = {10.1016/j.gep.2012.11.001},
file = {:Users/isabel/Documents/Papers/ChapterBookTunicata/Bibliography/ncRNAsintunicates/Kusakabeetal2013.pdf:pdf},
isbn = {1872-7298 (Electronic)$\backslash$r1567-133X (Linking)},
issn = {1567133X},
journal = {Gene Expression Patterns},
keywords = {Ascidian,MicroRNA,Myogenesis,Post-transcriptional regulation,Transcription start site},
number = {1-2},
pages = {43--50},
pmid = {23159539},
title = {{Characterization of the compact bicistronic microRNA precursor, miR-1/miR-133, expressed specifically in Ciona muscle tissues}},
url = {http://dx.doi.org/10.1016/j.gep.2012.11.001},
volume = {13},
year = {2013}
}
